{
  "font": {
    "family": "Archivo, sans-serif",
    "bodySize": "18px",
    "bodyLineHeight": "28.8px",
    "buttonSize": "16px",
    "h1Size": "83px",
    "h1LineHeight": "94.62px"
  },
  "colors": {
    "white": "#FFFFFF",
    "ink": "#24364E",
    "brandOrange": "#D34120",
    "brandOrangeHover": "#E07962"
  },
  "radii": {
    "button": "2px"
  },
  "layout": {
    "containerMax": "1210px",
    "navBreakpoint": "990px"
  },
  "button": {
    "paddingTop": "13px",
    "paddingRight": "15px",
    "paddingBottom": "12px",
    "paddingLeft": "15px",
    "letterSpacing": "1.6px",
    "textTransform": "uppercase",
    "transitionDuration": "0.2s",
    "transitionTiming": "ease-in-out",
    "transitionProps": "box-shadow, border-radius, background-color, opacity, color"
  }
}